{
  "expo": {
    "name": "WATTLY",
    "slug": "wattly",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "wattly",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e3a8a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swapnilpande.smartmeteroperations",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1e3a8a"
      },
      "package": "com.swapnilpande.smartmeteroperations",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "wattly"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ],
      "usesCleartextTraffic": true,
      "networkSecurityConfig": {
        "domain-config": [
          {
            "domain": "bff-alb-927359164.ap-south-1.elb.amazonaws.com",
            "includeSubdomains": true,
            "cleartextTrafficPermitted": true
          },
          {
            "domain": "sponge-balanced-cat.ngrok-free.app",
            "includeSubdomains": true,
            "cleartextTrafficPermitted": true
          }
        ]
      },
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_API_KEY_HERE"
        }
      }
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "ca18d21b-a06a-46f7-8f2b-fa31cab503d0"
      }
    },
    "owner": "swavedooja"
  }
}
